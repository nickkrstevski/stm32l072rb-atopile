from "generics/interfaces.ato" import SWD
from "generics/interfaces.ato" import USB2
from "generics/interfaces.ato" import SPI
from "generics/interfaces.ato" import I2C
from "generics/interfaces.ato" import Power

component STM32L072RBT6:
    # component STM32L072RBT6
    footprint = "LQFP-64_L10.0-W10.0-P0.50-LS12.0-BL"
    lcsc_id = "C915968"
    mpn = "C915968"
    # pins
    signal VDD ~ pin 1
    signal PC13 ~ pin 2
    signal PC14_minus_OSC32_IN ~ pin 3
    signal PC15_minus_OSC32_OUT ~ pin 4
    signal PH0_minus_OSC_IN ~ pin 5
    signal PH1_minus_OSC_OUT ~ pin 6
    signal NRST ~ pin 7
    signal PC0 ~ pin 8
    signal PC1 ~ pin 9
    signal PC2 ~ pin 10
    signal PC3 ~ pin 11
    signal VSSA ~ pin 12
    signal VDDA ~ pin 13
    signal PA0 ~ pin 14
    signal PA1 ~ pin 15
    signal PA2 ~ pin 16
    signal PA3 ~ pin 17
    signal VSS ~ pin 18
    VDD ~ pin 19
    signal PA4 ~ pin 20
    signal PA5 ~ pin 21
    signal PA6 ~ pin 22
    signal PA7 ~ pin 23
    signal PC4 ~ pin 24
    signal PC5 ~ pin 25
    signal PB0 ~ pin 26
    signal PB1 ~ pin 27
    signal PB2 ~ pin 28
    signal PB10 ~ pin 29
    signal PB11 ~ pin 30
    VSS ~ pin 31
    VDD ~ pin 32
    signal PB12 ~ pin 33
    signal PB13 ~ pin 34
    signal PB14 ~ pin 35
    signal PB15 ~ pin 36
    signal PC6 ~ pin 37
    signal PC7 ~ pin 38
    signal PC8 ~ pin 39
    signal PC9 ~ pin 40
    signal PA8 ~ pin 41
    signal PA9 ~ pin 42
    signal PA10 ~ pin 43
    signal PA11 ~ pin 44
    signal PA12 ~ pin 45
    signal PA13 ~ pin 46
    VSS ~ pin 47
    signal VDD_USB ~ pin 48
    signal PA14 ~ pin 49
    signal PA15 ~ pin 50
    signal PC10 ~ pin 51
    signal PC11 ~ pin 52
    signal PC12 ~ pin 53
    signal PD2 ~ pin 54
    signal PB3 ~ pin 55
    signal PB4 ~ pin 56
    signal PB5 ~ pin 57
    signal PB6 ~ pin 58
    signal PB7 ~ pin 59
    signal BOOT0 ~ pin 60
    signal PB8 ~ pin 61
    signal PB9 ~ pin 62
    VSS ~ pin 63
    VDD ~ pin 64
    signal gnd ~ VSS

    micro3v3 = new Power
    micro3v3.voltage = 1.8V to 3.6V
    micro3v3.gnd ~ gnd

    usb_power = new Power
    usb_power.vcc ~ VDD_USB
    usb_power.voltage = 3.0V to 3.6V
    usb_power.gnd ~ gnd

    usb = new USB2
    usb.dm ~ PA11
    usb.dp ~ PA12

    swd = new SWD
    swd.swdio ~ PA13
    swd.swclk ~ PA14

    spi1 = new SPI
    spi1.mosi ~ PA7
    spi1.miso ~ PA6
    spi1.sck ~ PA5
    spi1.cs ~ PA4
    spi1.gnd ~ gnd
    
    spi2 = new SPI
    spi2.mosi ~ PB15
    spi2.miso ~ PB14
    spi2.sck ~ PB13
    spi2.cs ~ PB12
    spi2.gnd ~ gnd

    i2c1 = new I2C
    i2c1.scl ~ PB6
    i2c1.sda ~ PB7
    i2c1.gnd ~ gnd

    i2c2 = new I2C
    i2c2.scl ~ PB10
    i2c2.sda ~ PB11
    i2c2.gnd ~ gnd

    i2c3 = new I2C
    i2c3.scl ~ PC0
    i2c3.sda ~ PC1
    i2c3.gnd ~ gnd
